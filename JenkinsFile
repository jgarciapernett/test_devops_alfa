pipeline {
  agent any
  options {
      buildDiscarder logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '5', daysToKeepStr: '', numToKeepStr: '5')
      disableConcurrentBuilds()
  }
stages {
        stage('Build') { 
            steps { 
                 echo "alfa_test prueba" 
            }
        }
      stage('Print Directory') {
            steps {
                script {
                    bat 'echo Current Directory: %CD%'
                }
            }
        }
  stage('Version Mave') {
            steps {
                script {
                     unstash 'app'
                     bat 'make check'
                }
            }
        }
        stage('Deploy') {
            steps {
                echo "alfa_test prueba"
            }
        }
    }
  } 
