pipeline {
  agent any
 
  tools {
        maven 'Maven 3.9.7'
        jdk 'jdk8'
    }
stages {
        stage('Build') { 
            steps { 
                 echo "alfa_test prueba" 
            }
        }
      stage('Print Directory') {
            steps {
                script {
                    bat 'echo Current Directory: %CD%'
                }
            }
        }
  stage('Version Mave') {
            steps {
                script {
                    bat 'mvn -version'
                }
            }
        }
        stage('Deploy') {
            steps {
                echo "alfa_test prueba"
            }
        }
    }
  } 
